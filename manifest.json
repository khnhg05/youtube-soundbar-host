{
  "manifest_version": 3,
  "name": "YouTube Option Bar",
  "version": "1.0",
  "description": "Thêm thanh option/soundboard trên YouTube",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
